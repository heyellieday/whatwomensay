<div class="panel panel-default">
	<div class="panel-body">
		<div class="col-md-4">
			<img class="img-responsive img-circle img-profile" src="{{profile_image_url}}" >
			<div class="caption">
				<h3>{{name}}</h3>
				<h4>{{location}}</h4>
				<p>{{description}}</p>
				<p><button id="checkLists" class="btn btn-default" role="button">Add to Lists</button</p>
			</div>
		</div>
		<div class="col-md-8">
			<h2 id="lists-header" class="hidden">Your Lists</h2>
			<ul id="lists" class="hidden list-group">
			</ul>
		</div>
	</div>
</div>